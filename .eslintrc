{
  "extends": "google",
  "env": {
    "node": true
  },
  "rules": {
    "no-var": 2,
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prefer-reflect": [2, {
      "exceptions": ["delete"]
    }],
    "prefer-template": 2,
    "prefer-spread": 2,
    "object-shorthand": [2, "always"],
    "babel/generator-star-spacing": 0,
    "babel/arrow-parens": 0,
    "babel/object-curly-spacing": 0,
    "generator-star-spacing": 0,
    "arrow-parens": 0,
    "object-curly-spacing": 0
  }
}
